{
  "name": "@emotionagency/utils",
  "version": "2.0.0",
  "description": "Useful utilities from the emotion-agency",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "type:build": "tsc --emitDeclarationOnly --declaration --declarationDir dist",
    "build": "vite build && npm run type:build",
    "pub": "npm run build && npm version patch && npm publish"
  },
  "keywords": [
    "js",
    "utilities",
    "javascript",
    "requestAnimationFrame",
    "resize",
    "raf",
    "clamp",
    "lerp"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letmewin22/utilities.git"
  },
  "author": "emotion-agency <hello@emotion-agency.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/letmewin22/utilities/issues"
  },
  "homepage": "https://github.com/letmewin22/utilities#readme",
  "devDependencies": {
    "@types/node": "^20.4.2",
    "typescript": "^5.1.6",
    "vite": "^5.0.0"
  }
}